<table id="example2" class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>No.</th>
            <th>Coin Name</th>
            <th>Coin Quantity</th>
            <th>Price</th>
            <th>Aksi</th>
        </tr>
    </thead>
    <tbody>
        <% for(let i = 0;i < nominals.length;i++){ %>
        <tr>
            <td><%= i+1 %></td>
            <td><%= nominals[i].coinName %></td>
            <td><%= nominals[i].coinQuantity %></td>
            <td><%= nominals[i].price %></td>
            <td>
                <form action="/nominal/destroy/<%= nominals[i]._id %>?_method=DELETE" method="POST">
                    <a href="/nominal/edit/<%= nominals[i]._id %>" class="btn btn-warning"><i class="fas fa-pencil-alt"></i> Ubah</a>
                    <button class="btn btn-danger"><i class="fas fa-trash"></i> Hapus</button>
                </form>
            </td>
        </tr>
        <% } %>
    </tbody>
    <tfoot>
        <tr>
            <th>No.</th>
            <th>Coin Name</th>
            <th>Coin Quantity</th>
            <th>Price</th>
            <th>Aksi</th>
        </tr>
    </tfoot>
</table>